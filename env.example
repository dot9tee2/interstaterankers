# EmailJS credentials (client-exposed). Copy this file to `.env.local` and fill in the values.
# Next.js exposes variables prefixed with `NEXT_PUBLIC_` to the client bundle.

NEXT_PUBLIC_EMAILJS_SERVICE_ID=your_service_id
NEXT_PUBLIC_EMAILJS_TEMPLATE_ID=your_template_id
NEXT_PUBLIC_EMAILJS_PUBLIC_KEY=your_public_key
